  "private": true,
  "main": "gulpfile.babel.js",
  "scripts": {
    "precommit": "npm run test",
    "revision": "gulp --gulpfile tools/gulpfile.babel.js revision",
    "header": "gulp --gulpfile tools/gulpfile.babel.js header",
    "start": "gulp --gulpfile tools/gulpfile.babel.js --env debug",
    "build": "gulp --gulpfile tools/gulpfile.babel.js --env production && npm run header && npm run revision",
    "lint": "eslint ./src/javascript",
    "test": "jasmine",
    "coverage": "nyc --extension .jsx --extension .es6 --reporter=lcov --reporter=text-lcov npm test",
    "pre-release": "npm run lint && npm run test",
    "create-helpers": "./node_modules/.bin/babel-external-helpers -t global > ./src/javascript/wrapper/helpers.js"
  },
  "pre-commit": [
    "pre-release"
  ],
  "engines": {
    "node": "7.1.0",
    "npm": "3.8.6"
  },
