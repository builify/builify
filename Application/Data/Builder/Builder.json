        {
          "type": "block.colors",
        }
      ]
    },
    {
      "id": "typography",
      "title": "sidetabs.typography.title",
      "subtitle": "sidetabs.typography.subtitle",
      "children": [
        {
          "type": "block.title",
          "title": "design.titles.misc"
        },
        {
          "type": "block.switch",
          "state": "off",
          "name": "design.titles.verticalrythm",
          "action": "toggle.baseline"
