{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false
    }
  },
  "plugins": [
    "react"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "strict": 0,
    "comma-dangle": 0,
    "object-curly-spacing": 0,
    "require-jsdoc": 0,
    "no-console": 0,
    "max-len": 0,
    "indent": [2, 4]
  }
}
